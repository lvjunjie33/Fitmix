.sub-nav-margin {
  height: 10px;
  background-color: #999999;
}
.sub-nav {
  background-color: #3d3d3e;
  height: 50px;
  & ul  {
    margin-bottom: 0px;
    margin-top: 0px;
    height: 50px;
    li {
      list-style: none;
      float: left;
      margin: 0 10px;
      height: 50px;
      line-height: 50px;
      &:last-child {
        float: right;
        margin-right: 150px;
      }
      a {
        color: #ffffff;
        vertical-align: middle;
      }
    }

  }
}