package com.business.core.entity.runPlan;

/**
 * Created by weird on 2016/6/16.
 */
public class Stages {

    /**
     *  状态：未定义
     */
    public static final Integer STATE_UNDEFINE = -1;

    /**
     *  状态：执行中
     */
    public static final Integer STATE_DEFAULT = 0;

    /**
     *  状态:成功
     */
    public static final Integer STATE_SUCCESS = 1;

    /**
     *  状态:失败
     */
    public static final Integer STATE_DEFEAT = 2;

    /**
     *  预备阶段
     */
    public static final Integer STAGE_PREPARE = 0;

    /**
     *  提高速度阶段
     */
    public static final Integer STAGE_INCREASESPEED = 1;

    /**
     *  进一步提高阶段
     */
    public static final Integer STAGE_FURTHERINCREASE = 2;

    /**
     * 模拟阶段
     */
    public static final Integer STAGE_GAMESIMULATION = 3;

    /**
     *  比赛阶段
     */
    public static final Integer STAGE_PREPARATIONANDCOMPETITION = 4;

    /**
     *  恢复阶段
     */
    public static final Integer STAGE_RECOVER = 5;

    /**
     *  阶段名
     */
    private String stageName;

    /**
     *  阶段类型
     */
    private Integer stageType;

    /**
     *  日期
     */
    private Integer date;

    /**
     *  详细时间 格式:yyyy-MM-dd
     */
    private String dateTime;

    /**
     *  当天的时间戳
     */
    private Long dateLong;

    /**
     *  是否完成
     *  0、未完成 1、已完成
     */
    private Integer state;

    /**
     *  内容
     */
    private String stageContent;

    /**
     *  该阶段的跑步距离
     */
    private Double distance;

    /**
     *  该内容的配速
     */
    private String stageSpeed;

    /**
     *  该内容的耗时
     */
    private String stageTime;

    /**
     *  内容介绍
     */
    private String contentDescription;

    public String getStageName() {
        return stageName;
    }

    public void setStageName(String stageName) {
        this.stageName = stageName;
    }

    public Integer getStageType() {
        return stageType;
    }

    public void setStageType(Integer stageType) {
        this.stageType = stageType;
    }

    public Integer getDate() {
        return date;
    }

    public void setDate(Integer date) {
        this.date = date;
    }

    public Integer getState() {
        return state;
    }

    public void setState(Integer state) {
        this.state = state;
    }

    public String getDateTime() {
        return dateTime;
    }

    public void setDateTime(String dateTime) {
        this.dateTime = dateTime;
    }

    public Long getDateLong() {
        return dateLong;
    }

    public void setDateLong(Long dateLong) {
        this.dateLong = dateLong;
    }

    public String getStageContent() {
        return stageContent;
    }

    public void setStageContent(String stageContent) {
        this.stageContent = stageContent;
    }

    public Double getDistance() {
        return distance;
    }

    public void setDistance(Double distance) {
        this.distance = distance;
    }

    public String getStageSpeed() {
        return stageSpeed;
    }

    public void setStageSpeed(String stageSpeed) {
        this.stageSpeed = stageSpeed;
    }

    public String getStageTime() {
        return stageTime;
    }

    public void setStageTime(String stageTime) {
        this.stageTime = stageTime;
    }

    public String getContentDescription() {
        return contentDescription;
    }

    public void setContentDescription(String contentDescription) {
        this.contentDescription = contentDescription;
    }
}
