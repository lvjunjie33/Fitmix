    TaskType() {
        this.coin = 0;
        this.status = TaskInfo.STATUS_INVALID;
    }

    TaskType(Integer taskType, String desc) {
        this.taskType = taskType;
        this.coin = 0;
        this.status = TaskInfo.STATUS_INVALID;
        this.desc = desc;
    }

    public Integer getCoin() {
        return coin;
    }

    public void setCoin(Integer coin) {
        this.coin = coin;
    }

    public Integer getStatus() {
