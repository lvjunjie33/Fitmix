<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<div id="des-add" class="uk-modal up-open">
    <div class="uk-modal-dialog">
        <button type="button" class="uk-modal-close uk-close"></button>
        <h1>Description Add</h1>
        <div class="uk-form-row">
            <form action="/run-plan/des-add.json" method="POST" class="uk-form" id="des-add-form" isform>
                <div class="uk-form-row">
                    <h4>关键词</h4>
                    <input class="uk-width-2-3 uk-form-large" type="text" name="key" placeholder="关键词" notnull />
                </div>
                <div class="uk-form-row">
                    <h4>描述</h4>
                    <textarea name="des" class=" uk-width-2-3 uk-form-large" placeholder="des"></textarea>
                </div>
                <div class="uk-form-row uk-text-right">
                    <button class="uk-button uk-button-primary" id="des-add-submit">add</button>
                </div>
            </form>
        </div>
    </div>
</div>
