public class ErrerCode {

    /**
     * 错误名称
     */
    private String name;
    /**
     * 错误代码
     */
    private String code;
    /**
     * 错误描述
     */
    private String desc;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getDesc() {
        return desc;
    }

    public void setDesc(String desc) {
        this.desc = desc;
    }
}
