package com.business.core.entity.user;

public class UserRunStatistics {
    public static final Integer TEN_MILE_ABILITY_LEVEL_DEFAULT = 0;
    public static final Integer TEN_MILE_ABILITY_LEVEL_ONE = 1;
    public static final Integer TEN_MILE_ABILITY_LEVEL_TWO = 2;
    public static final Integer TEN_MILE_ABILITY_LEVEL_THREE = 3;

    public static final Integer HALF_MARATHON_ABILITY_LEVEL_DEFAULT = 0;
    public static final Integer HALF_MARATHON_ABILITY_LEVEL_ONE = 1;
    public static final Integer HALF_MARATHON_ABILITY_LEVEL_TWO = 2;
    public static final Integer HALF_MARATHON_ABILITY_LEVEL_THREE = 3;

    private Integer id;

    private Integer uid;

    private Long distance;

    private Integer tenMileAbility;

    private Integer halfMarathonAbility;

    private String runLevel;

    private Long addTime;

    private Long modifyTime;

    private RunLevelInfo runLevelInfo;

    public UserRunStatistics(Integer uid, Long distance, Integer tenMileAbility, Integer halfMarathonAbility, String runLevel, Long addTime, Long modifyTime) {
        this.uid = uid;
        this.distance = distance;
        this.runLevel = runLevel;
        this.tenMileAbility = tenMileAbility;
        this.halfMarathonAbility = halfMarathonAbility;
        this.addTime = addTime;
        this.modifyTime = modifyTime;
    }

    public UserRunStatistics() {
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getUid() {
        return uid;
    }

    public void setUid(Integer uid) {
        this.uid = uid;
    }

    public Long getDistance() {
        return distance;
    }

    public void setDistance(Long distance) {
        this.distance = distance;
    }

    public Integer getTenMileAbility() {
        return tenMileAbility;
    }

    public void setTenMileAbility(Integer tenMileAbility) {
        this.tenMileAbility = tenMileAbility;
    }

    public Integer getHalfMarathonAbility() {
        return halfMarathonAbility;
    }

    public void setHalfMarathonAbility(Integer halfMarathonAbility) {
        this.halfMarathonAbility = halfMarathonAbility;
    }

    public Long getAddTime() {
        return addTime;
    }

    public void setAddTime(Long addTime) {
        this.addTime = addTime;
    }

    public Long getModifyTime() {
        return modifyTime;
    }

    public void setModifyTime(Long modifyTime) {
        this.modifyTime = modifyTime;
    }

    public String getRunLevel() {
        return runLevel;
    }

    public void setRunLevel(String runLevel) {
        this.runLevel = runLevel;
    }

    public RunLevelInfo getRunLevelInfo() {
        return runLevelInfo;
    }

    public void setRunLevelInfo(RunLevelInfo runLevelInfo) {
        this.runLevelInfo = runLevelInfo;
    }
}