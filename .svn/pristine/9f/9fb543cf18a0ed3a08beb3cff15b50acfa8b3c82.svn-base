body {
    margin: 0;
    padding: 0;
    background-color: #131218;
    SCROLLBAR-FACE-COLOR: #BED8EB;
    SCROLLBAR-SHADOW-COLOR: #DDF8FF;
    SCROLLBAR-HIGHLIGHT-COLOR: #92C0D1;
    SCROLLBAR-3DLIGHT-COLOR: #DDF8FF;
    SCROLLBAR-DARKSHADOW-COLOR: #92C0D1;
    SCROLLBAR-TRACK-COLOR: #BED8EB;
    SCROLLBAR-ARROW-COLOR: #92C0D1;
}

@keyframes mysnow{
    0%{
        bottom:110%;

    }
    50%{

        transform: rotate(180deg);
    }
    100%{
        transform: rotate(0deg);

        bottom:-20%;
    }
}
@-webkit-keyframes mysnow{
    0%{
        bottom:110%;
    }
    50%{
        -webkit-transform: rotate(180deg);
    }
    100%{
        -webkit-transform: rotate(0deg);
        bottom:-20%;
    }
}

@-moz-keyframes mysnow{
    0%{
        bottom:110%;

    }
    50%{

        -moz-transform: rotate(180deg);
    }
    100%{
        -moz-transform: rotate(0deg);
        bottom:-20%;
    }
}
@-ms-keyframes mysnow{
    0%{
        bottom:110%;

    }
    50%{

        -ms-transform: rotate(180deg);
    }
    100%{
        -ms-transform: rotate(0deg);

        bottom:-20%;
    }
}
@-o-keyframes mysnow{
    0%{
        bottom:110%;

    }
    50%{

        -o-transform: rotate(180deg);
    }
    100%{
        -o-transform: rotate(0deg);

        bottom:-20%;
    }
}

.roll{
    position:absolute;
    opacity:1;
    animation: mysnow 10s ;
    -webkit-animation: mysnow 10s ;
    -moz-animation: mysnow 10s ;
    -ms-animation: mysnow 10s ;
    -o-animation: mysnow 10s ;
    height:1.8rem;
}
